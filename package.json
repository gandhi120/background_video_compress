{
  "name": "backgroundVideoCompress",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "common": "rm -rf node_modules/ && sleep 10 && npm install --legacy-peer-deps && npm run podSetup",
    "podInstall": "cd ios && pod install && cd ../",
    "cleanGradle": "cd android && ./gradlew clean && cd ../",
    "podUnInstall": "cd ios && pod deintegrate && cd ../",
    "podSetup": "npm run podUnInstall && npm run podInstall"
  },
  "dependencies": {
    "@react-native-camera-roll/camera-roll": "^7.9.0",
    "@react-navigation/native": "7.0.14",
    "@react-navigation/native-stack": "7.2.0",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-permissions": "^5.2.1",
    "react-native-safe-area-context": "5.0.0",
    "react-native-screens": "4.4.0",
    "react-native-video": "^6.8.2",
    "react-native-vision-camera": "4.6.3",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
